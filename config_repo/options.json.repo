[
{
"name" : "daynighttab========================================",
"label" : "Day/Night Settings",
"type" : "header-tab"
},
{
"name" : "daytimesettingsheader",
"label" : "Daytime settings",
"type" : "header"
},
{
"name" : "daynightcolumns",
"label" : "Setting, Day Value, Description",
"candrop" : "Description",
"type" : "header-column"
},
{
"name" : "_takedaytimeimages",
"default" : true,
"description" : "Enable to <strong>take</strong> daytime images.",
"label" : "Daytime Capture",
"type" : "boolean",
"checkchanges" : true,
"action" : "reload",
"advanced" : true
},
{
"name" : "_savedaytimeimages",
"default" : false,
"description" : "Enable to <strong>save</strong> daytime images.",
"label" : "Daytime Save",
"type" : "boolean",
"booldependson" : "takedaytimeimages",
"advanced" : true
},
{
"name" : "_dayautoexposure",
"default" : true,
"description" : "Enable to use daytime auto-exposure.",
"label" : "Auto-Exposure",
"type" : "boolean",
"booldependson" : "takedaytimeimages",
"action" : "reload",
"advanced" : true
},
{
"name" : "_daymaxautoexposure",
"minimum" : "_min",
"maximum" : "_max",
"default" : "_default",
"description" : "Maximum daytime Auto-Exposure time in milliseconds (1000ms = 1 sec).",
"label" : "Max Auto-Exposure",
"type" : "float",
"booldependson" : "takedaytimeimages AND daymaxautoexposure",
"action" : "reload"
},
{
"name" : "_dayexposure",
"minimum" : "_min",
"maximum" : "_max",
"default" : "day_default",
"description" : "Manual exposure time in milliseconds (1000ms = 1 sec). Can be a fraction.<br>If using daytime <span class='WebUISetting'>Auto-Exposure</span>, this number is used as a starting point.",
"label" : "Manual Exposure",
"type" : "float",
"booldependson" : "takedaytimeimages",
"action" : "reload",
"advanced" : true
},
{
"name" : "_daymean",
"minimum" : 0.0,
"maximum" : 1.0,
"default" : "day_default",
"description" : "The target mean brightness level when <span class='WebUISetting'>Auto-Exposure</span> is on. <span class='WebUIValue'>1.0</span> is pure white.<br>Best used when <span class='WebUISetting'>Auto-Exposure</span> and <span class='WebUISetting'>Auto-Gain</span> are on.<br><span class='WebUIValue'>0</span> disables this auto-exposure mode.",
"label" : "Mean Target",
"type" : "float",
"booldependson" : "takedaytimeimages",
"action" : "reload",
"advanced" : true
},
{
"name" : "_daymeanthreshold",
"minimum" : "day_min",
"maximum" : "day_max",
"default" : "day_default",
"description" : "When using <span class='WebUISetting'>Mean Target</span> this specifies how close to the target the brightness should be.",
"label" : "Mean Threshold",
"type" : "float",
"booldependson" : "takedaytimeimages AND daymean",
"action" : "reload",
"advanced" : true
},
{
"name" : "_daydelay",
"minimum" : "_min",
"maximum" : "none",
"default" : 30000,
"description" : "Delay between daytime images in milliseconds (1000ms = 1 sec).",
"label" : "Delay",
"type" : "integer",
"booldependson" : "takedaytimeimages",
"action" : "reload"
},
{
"name" : "_dayautogain",
"default" : false,
"description" : "Enable to use dayime Auto-Gain.",
"label" : "Auto-Gain",
"type" : "boolean",
"booldependson" : "takedaytimeimages",
"action" : "reload",
"advanced" : true
},
{
"name" : "_daymaxautogain",
"minimum" : "_min",
"maximum" : "_max",
"default" : "_default",
"description" : "Maximum gain when using <span class='WebUISetting'>Auto-Gain</span>.",
"label" : "Max Auto-Gain",
"type" : "float",
"booldependson" : "takedaytimeimages AND dayautogain",
"action" : "reload",
"advanced" : true
},
{
"name" : "_daygain",
"minimum" : "_min",
"maximum" : "_max",
"default" : "day_default",
"description" : "Manually sets the light sensitivity of the camera.<br>A high number returns a brighter image but more noise. Can normally leave at the minimum value for daytime.",
"label" : "Gain",
"type" : "float",
"booldependson" : "takedaytimeimages",
"action" : "reload",
"advanced" : true
},
{
"name" : "_daybin",
"default" : 1,
"description" : "Binning level. <span class='WebUIValue'>1x1</span> = OFF.",
"label" : "Binning",
"type" : "select",
"options" : [
	"bin_values"
],
"booldependson" : "takedaytimeimages",
"action" : "reload",
"advanced" : true
},
{
"name" : "_dayawb",
"default" : true,
"description" : "Sets Auto White Balance (camera adjusts color).",
"label" : "Auto White Balance",
"type" : "boolean",
"display" : "_display",
"booldependson" : "takedaytimeimages",
"action" : "reload"
},
{
"name" : "_daywbr",
"minimum" : "_min",
"maximum" : "_max",
"default" : "_default",
"description" : "Red component for the white balance.<br>When using <span class='WebUISetting'>Auto White Balance</span>, this number is a starting point.",
"label" : "Red Balance",
"type" : "float",
"display" : "_display",
"booldependson" : "takedaytimeimages AND dayawb",
"action" : "reload"
},
{
"name" : "_daywbb",
"minimum" : "_min",
"maximum" : "_max",
"default" : "_default",
"description" : "Blue component for the white balance.<br>When using <span class='WebUISetting'>Auto White Balance</span>, this number is a starting point.",
"label" : "Blue Balance",
"type" : "float",
"display" : "_display",
"booldependson" : "takedaytimeimages AND dayawb",
"action" : "reload"
},
{
"name" : "_dayskipframes",
"minimum" : 0,
"maximum" : "none",
"default" : 5,
"description" : "When starting Allsky during the day, skip up to this many frames while the software gets to the correct exposure.",
"label" : "Frames To Skip",
"type" : "integer",
"booldependson" : "takedaytimeimages AND dayautoexposure",
"action" : "reload",
"advanced" : true
},
{
"name" : "_dayenablecooler",
"default" : true,
"description" : "Enable to use cooling (on cooled cameras only).",
"label" : "Cooling",
"type" : "boolean",
"display" : "_display",
"booldependson" : "takedaytimeimages",
"action" : "reload",
"advanced" : true
},
{
"name" : "_daytargettemp",
"minimum" : "_min",
"maximum" : "_max",
"default" : "_default",
"description" : "Sensor's target temperature <i>when cooler is enabled</i> (degrees Celsius).",
"label" : "Target Temp.",
"type" : "integer",
"display" : "_display",
"booldependson" : "takedaytimeimages AND dayenablecooler",
"action" : "reload",
"advanced" : true
},
{
"name" : "_daytuningfile",
"default" : "",
"description" : "Name of the optional day camera tuning file to use.<br>See <a href='https://www.raspberrypi.com/documentation/accessories/camera.html' target='_blank' rel='noreferrer' title='Opens in new tab/window'>documentation <i class='fa fa-external-link-alt fa-fw'></i></a>, <a href='https://datasheets.raspberrypi.com/camera/raspberry-pi-camera-guide.pdf' target='_blank' rel='noreferrer' title='Opens in new tab/window'>document <i class='fa fa-external-link-alt fa-fw'></i></a>, or the camera manual for more information.",
"label" : "Tuning File",
"type" : "widetext",
"display" : "_display",
"checkchanges" : true,
"optional" : true,
"booldependson" : "takedaytimeimages",
"action" : "restart",
"advanced" : true
},

